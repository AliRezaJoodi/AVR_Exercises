#include <sensor_thermistor.h>

//Pull-Up NTC
/*
VCC
R1=NTC
R2=1K5
GND
*/
//********************************************************
float Thermistor_ConvertVoltToOhm_PullUp(float vr2){
    float y=0;
    
    y=R2*(VCC-vr2)/vr2; 
    return y;
}

